{
    "name": "gdpa/chabok",
    "description": "Chabok Notifications driver",
    "homepage": "https://github.com/gdpa/chabok",
    "license": "MIT",
    "authors": [
        {
            "name": "Morteza Poussaneh",
            "email": "morteza.poussaneh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^7.3|^7.4|^8.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/notifications": "5.*",
        "illuminate/support": "~5.5.0 || ~5.6.0 || ~5.7.0 || ~5.8.0 || ^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.4",
        "orchestra/testbench": "^3.5 || ^3.6 || ^3.7 || ^3.8 || ^4.0 || ^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Chabok\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Chabok\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
